<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:key="notification_settings"
        android:title="알림설정" >
        <PreferenceScreen
            android:key="parking_notification"
            android:title="주차알림"
            android:summary="출차 알림 시간을 선택하세요.">
            <intent
                android:targetClass="com.example.athome.setting.SettingParkingActivity"
                android:targetPackage="com.example.athome"
              />
        </PreferenceScreen>

        <PreferenceScreen
            android:key="parking_lot_notification"
            android:title="주차장알림"
            android:summary="내 공유주차장 예약 알림을 설정하세요.">
            <intent
                android:targetClass="com.example.athome.setting.SettingParkingLotActivity"
                android:targetPackage="com.example.athome"
                />

        </PreferenceScreen>

    </PreferenceCategory>

    <PreferenceCategory
        android:key="terms_of_service_title"
        android:title="약관 및 방침" >
        <Preference
            android:key="terms_of_service"
            android:title="이용약관" />
        <Preference
            android:key="privacy"
            android:title="개인정보 취급방침" />
        <Preference
            android:key="location-based"
            android:title="위치기반 서비스 이용약관" />
    </PreferenceCategory>

    <PreferenceCategory
        android:key="app_version_title"
        android:title="앱 버전" >
        <Preference
            android:key="app_version"
            android:title="1.0.0" />
    </PreferenceCategory>

    <PreferenceCategory
        android:key="account_delete_title"
        android:title="회원 탈퇴">
        <Preference
            android:key="account_delete"
            android:title="회원 탈퇴"/>
    </PreferenceCategory>
</PreferenceScreen>